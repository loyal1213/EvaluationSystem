	<!--缺少建筑物-->
	<!--演习规划区-->
	<model name="facelayer1" driver="feature_geom" opacity="0.65">
		<features name="world" driver="ogr">
			<url>../TrainingArea/</url>
			<buffer distance="-0.05" />
		</features>
		<styles>
			<style type="text/css">
                default {
		    fill:          #6cb1ff;
                    stroke:		   #0000c8;
                    stroke-width:  0px;
		altitude-clamping:terrain-drape;
                }
			</style>
		</styles>
	</model>
